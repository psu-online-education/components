{% set regions = [] %}
{% for i in 1..5 %}
  {% set region %}
    {% include '@oe/callout/callout.twig' with {
      background: 'light-grey',
      content: 'Region ' ~ i ~ ' content',
    } only %}
  {% endset %}
  {% set regions = regions|merge([{ weight: 20, content: region }]) %}
{% endfor %}
{% include '@oe/layout-multi-col/layout-multi-col.twig' with {
    regions: regions,
} only %}
