{% embed '@oe/layout-two-col/layout-two-col.twig' %}
  {% block first %}
    {% embed '@oe/layout-two-col/layout-two-col.twig' %}
      {% block first %}
        {% include '@oe/callout/callout.twig' with {
          background: 'light-grey',
          content: 'First inner layout first region content'
        } only %}
      {% endblock %}
      {% block second %}
        {% include '@oe/callout/callout.twig' with {
          background: 'light-grey',
          content: 'First inner layout second region content'
        } only %}
      {% endblock %}
    {% endembed %}
  {% endblock %}
  {% block second %}
    {% embed '@oe/layout-two-col/layout-two-col.twig' %}
      {% block first %}
        {% include '@oe/callout/callout.twig' with {
          background: 'blue-gradient',
          content: 'Second inner layout first region content',
        } only %}
      {% endblock %}
      {% block second %}
        {% include '@oe/callout/callout.twig' with {
          background: 'blue-gradient',
          content: 'Second inner layout second region content',
        } only %}
      {% endblock %}
    {% endembed %}
  {% endblock %}
{% endembed %}
