{% embed '@oe/layout-two-col/layout-two-col.twig' with { first_width: 75, second_width: 25 } only %}
  {% block first %}
    {% include '@oe/callout/callout.twig' with {
      background: 'light-grey',
      content: 'First region content'
    } only %}
