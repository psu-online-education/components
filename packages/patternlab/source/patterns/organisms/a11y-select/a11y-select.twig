    <option value="MEMTMLGCT">Technical Management and Leadership Essentials (Graduate Certificate)</option>
  </select>
</label>
<script>
  const dependent_degrees_model = {
    bachelors: {
      SEBCBS: 'Software Engineering (Bachelor of Science)'
    },
    masters: {
      ACS: 'Acoustics (Master of Engineering)',
      AMDMENG: 'Additive Manufacturing and Design (Master of Engineering)',
      EEMEN: 'Electrical Engineering (Master of Engineering)',
      ENGMT: 'Engineering Management (Master\'s Degree)',
      IEMENG: 'Industrial Engineering (Master of Engineering)',
      MSME: 'Mechanical Engineering (Master of Science)',
      'NUC E': 'Nuclear Engineering (Master of Engineering)',
      SWENG: 'Software Engineering (Master\'s Degree)',
      SYSEN: 'Systems Engineering (Master of Engineering)'
    },
    doctoral: {
      ENGRDENG: 'Engineering (Doctor of Engineering)'
    },
    gradcerts: {
      AMDGCT: 'Additive Manufacturing and Design (Graduate Certificate)',
      MSEAIEGCT: 'AI Engineering (Graduate Certificate)',
      MSEDAEGCT: 'Data Analytics and Engineering (Graduate Certificate)',
      EEPCT: 'Electrical Engineering (Postbaccalaureate Certificate)',
      ELIMGCT: 'Engineering Leadership and Innovation Management (Graduate Certificate)',
      MEMLCDGCT: 'Life-Cycle System Design (Graduate Certificate)',
      MSEPQMGCT: 'Project Quality Management (Graduate Certificate)',
      MEMQMGCT: 'Quality Management Across Product Lifecycle (Graduate Certificate)',
      MEMQDMGCT: 'Quantitative Decision-Making for Engineering Managers (Graduate Certificate)',
      MSESADGCT: 'Software Architecture and Design (Graduate Certificate)',
      MENGSMGCT: 'System Modeling and Analysis (Graduate Certificate)',
      MENGSQGCT: 'System Quality &amp; Risk Evaluation (Graduate Certificate)',
      MENGTMGCT: 'Technical Management (Graduate Certificate)',
      MEMTMLGCT: 'Technical Management and Leadership Essentials (Graduate Certificate)',
    },
  };
  const programs = document.getElementById('a11y-select-programs');
  document.getElementById('a11y-select-degree-level').addEventListener('change', e => {

    let options = new Map();
    options.set(programs.options[programs.selectedIndex].value, programs.options[programs.selectedIndex].text)
    options.set('', 'Select a program');
    if (e.target.value === 'All') {
      for (const group in dependent_degrees_model) {
        for (const code in dependent_degrees_model[group]) {
          options.set(code, dependent_degrees_model[group][code]);
        }
      }
    }
    else {
      for (const key in dependent_degrees_model[e.target.options[e.target.selectedIndex].value]) {
        options.set(key, dependent_degrees_model[e.target.options[e.target.selectedIndex].value][key]);
      }
    }
    let new_html = '';
    options.forEach((program, code) => {
      new_html += '<option value="' + code + '">' + program + '</option>';
    });
    programs.innerHTML = new_html;
  });
</script>
