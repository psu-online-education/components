{% set tables %}
    <div class="a11y-table">
    <table class="a11y-table__table">
      <caption class="a11y-table__caption">Tuition rates for the fall 2024, spring 2025, and summer 2025 semesters.</caption>
      <thead class="a11y-table__thead">
        <tr class="a11y-table__tr">
          <th class="a11y-table__th">How many credits do you plan to take per semester?</th>
          <th class="a11y-table__th"><strong>If you have 59 or fewer credits</strong></th>
          <th class="a11y-table__th"><strong>If you have 60 or more credits</strong></th>
        </tr>
      </thead>
      <tbody class="a11y-table__tbody">
      <tr class="a11y-table__tr">
        <td class="a11y-table__td">11 or fewer</td>
        <td class="a11y-table__td">$632 per credit</td>
        <td class="a11y-table__td">$678 per credit</td>
      </tr>
      <tr class="a11y-table__tr">
        <td class="a11y-table__td">12â€“19</td>
        <td class="a11y-table__td">$7,678 per semester</td>
        <td class="a11y-table__td">$8,288 per semester</td>
      </tr>
      </tbody>
    </table>
  </div>
  <br><br>
  <div class="a11y-table">
    <table class="a11y-table__table">
      <thead class="a11y-table__thead">
        <tr class="a11y-table__tr">
          {% for i in 1..15 %}
            <th class="a11y-table__th" style="white-space: nowrap">Table header {{ i }}</th>
          {% endfor %}
        </tr>
      </thead>
      <tbody class="a11y-table__tbody">
        {% for i in 1..15 %}
          <tr class="a11y-table__tr">
            {% for j in 1..15 %}
              <td class="a11y-table__td" style="white-space: nowrap">Table data ({{ i }}, {{ j }})</td>
            {% endfor %}
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
{% endset %}

{% set callout %}
  {% include '@oe/callout/callout.twig' with {
  content: tables,
} only %}
{% endset %}

{% include '@oe/background-container/background-container.twig' with {
  color: 'light-grey',
  content: callout,
} only %}